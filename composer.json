{
    "name": "fieryprophet/PHPSandbox",
    "description": "A PHP library that can be used to run PHP code in a sandbox environment",
    "keywords": ["php", "sandbox"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Elijah Horton"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fieryprophet/PHPSandbox"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "nikic/PHP-Parser": "*",
        "jeremeamia/FunctionParser": "*"
    },
    "autoload": {
        "psr-0": { "PHPSandbox": "src/" }
    }
}